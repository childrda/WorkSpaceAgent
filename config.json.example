{
  "poll_interval_minutes": 5,
  "geo_db_path": "/opt/mcp_agent/GeoLite2-City.mmdb",
  "domain": "yourdomain.com",
  "log_level": "INFO",
  "google": {
    "admin_email": "admin@yourdomain.com",
    "use_alert_center": true,
    "max_alerts": 50
  },
  "alerts": {
    "enable_email": true,
    "alert_subject_prefix": "[SECURITY ALERT]"
  },
  "drive": {
    "enabled": true
  },
  "security": {
    "impossible_travel_threshold_mph": 500,
    "allowed_states": ["VA", "Virginia"],
    "state_check_enabled": true
  },
  "phishing": {
    "impersonation_keywords": ["superintendent", "principal", "superintendant", "prinicipal"],
    "leadership_keywords": ["finance", "hr", "human resources", "chief", "director", "executive"],
    "suspicious_extensions": [".exe", ".scr", ".bat", ".zip", ".js", ".vbs", ".cmd"],
    "public_sharing_indicators": [
      "anyoneWithLink",
      "anyone_with_link",
      "anyone",
      "public",
      "anyoneWithTheLink",
      "anyone_with_the_link"
    ],
    "log_all_drive_events": false
  },
  "dashboard": {
    "display_timezone": "America/New_York",
    "stats_lookback_days": 7,
    "refresh_interval_seconds": 30,
    "query_limits": {
      "impossible_travel": 10,
      "recent_logins": 10,
      "phishing_alerts": 10,
      "phishing_emails": 10,
      "login_pairs": 2
    }
  },
  "retention": {
    "retention_days": 180,
    "archive_path": "/opt/mcp_agent/archives",
    "enable_archiving": true
  },
  "gmail": {
    "enabled": true,
    "mailbox": "security-alerts@yourdomain.com",
    "poll_lookback_minutes": 10,
    "max_messages_per_poll": 50,
    "include_spam": false,
    "allowed_sender_domains": ["yourdomain.com"],
    "trusted_file_domains": ["yourdomain.com"],
    "high_risk_display_names": [
      "superintendent",
      "chief financial officer",
      "cfo",
      "principal",
      "finance director"
    ],
    "urgency_keywords": [
      "urgent",
      "immediate action",
      "wire transfer",
      "gift card",
      "payroll",
      "w-2",
      "bank account"
    ],
    "financial_keywords": [
      "wire",
      "invoice",
      "payment",
      "gift card",
      "bitcoin",
      "bank details"
    ],
    "share_link_domains": [
      "docs.google.com",
      "drive.google.com",
      "forms.gle",
      "forms.google.com",
      "sharepoint.com",
      "onedrive.live.com",
      "1drv.ms",
      "dropbox.com",
      "box.com"
    ]
  },
  "phishing_detection": {
    "combined_confidence_threshold": 0.75
  }
}

